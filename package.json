  "private": true,
  "scripts": {
    "____________________BUILD AND RUN______________________________________": "",
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "start:dev": "next dev",
    "start:prod": "next start",
    "____________________CODEGEN____________________________________________": "",
    "extract-translations": "npx i18next 'app/**/*.{tsx,ts}' -c i18next-parser.config.ts",
    "____________________HTTPS_______________________________________________": "",
    "https-dev": "npx next dev --experimental-https --experimental-https-key ./localhost.key --experimental-https-cert ./localhost.crt --experimental-https-ca ./root_ca.crt",
    "____________________LINTING AND FORMATTING_____________________________": "",
    "check": "npm run lint:style && npm run ts:check && npm run lint",
    "lint": "npx eslint \"**/*.{js,ts,jsx,tsx,cjs,cts,mjs,mts}\" .",
    "lint:fix": "npm run lint -- --fix",
    "lint:style": "stylelint \"**/*.{css,scss}\"",
    "lint:style:fix": "npm run lint:style -- --fix",
    "ts:check": "tsc --noEmit",
    "____________________TESTING___________________________________________": "",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "test": "npm run check && npm run jest",
    "____________________STORYBOOK__________________________________________": "",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
